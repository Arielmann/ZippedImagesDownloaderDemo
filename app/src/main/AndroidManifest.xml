<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.zippedimagesdownloaderdemo">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".app.ZippedImagesDownloadApp"
        android:icon="@mipmap/sweetch_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/sweetch_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ZippedImagesDownloaderDemo"
        tools:ignore="AllowBackup">
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>